{
  "name": "node-template",
  "version": "1.0.0",
  "description": "My nodejs template as a blog server",
  "directories": {
    "lib": "src"
  },
  "main": "lib",
  "engines": {
    "node": ">=7"
  },
  "scripts": {
    "build:clean": "rimraf lib",
    "build": "babel src -d lib",
    "doc": "documentation build src/** --format html --github --document-exported --output doc/api",
    "prebuild": "npm run build:clean",
    "start": "node-dev src",
    "start-prod": "NODE_ENV=production npm run build & node lib",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tuanlao/node-template.git"
  },
  "author": "TuanLV",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuanlao/node-template/issues"
  },
  "homepage": "https://github.com/tuanlao/node-template#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@feathersjs/authentication": "^4.5.2",
    "@feathersjs/authentication-local": "^4.5.2",
    "@feathersjs/authentication-oauth": "^4.5.2",
    "@feathersjs/configuration": "^4.5.2",
    "@feathersjs/errors": "^4.5.2",
    "@feathersjs/express": "^4.5.2",
    "@feathersjs/feathers": "^4.5.2",
    "@feathersjs/transport-commons": "^4.5.2",
    "acorn": "^7.1.1",
    "ajv": "^6.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "feathers-hooks-common": "^5.0.2",
    "feathers-logger": "^0.3.2",
    "feathers-mongoose": "^8.3.0",
    "feathers-reactive": "^0.8.1",
    "forever": "^2.0.0",
    "helmet": "^3.21.3",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "mongoose": "^5.8.11",
    "source-map-support": "^0.5.16",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-source-map-support": "^2.1.1",
    "cross-env": "^7.0.2",
    "documentation": "^12.1.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "expect-more-jest": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^25.2.2",
    "jest-extended": "^0.11.5",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^1.0.2",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.9",
    "node-dev": "^4.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1"
  }
}
